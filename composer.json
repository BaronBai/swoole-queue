{
    "name": "yaochufa/queue",
    "type": "library",
    "description": "基于swoole的抢购排队通用中间件，适合抢购秒杀场景，跟具体业务解耦",
    "keywords": [
        "yaochufa",
        "queue"
    ],
    "homepage": "https://github.com/YaochufaTech/swoole-queue",
    "license": "MIT",
    "authors": [
        {
            "name": "kcloze",
            "email": "pei.greet@gmail.com",
            "homepage": "https://github.com/YaochufaTech/swoole-queue",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~6.5"
    },
    "autoload": {
        "psr-4": {
            "Yaochufa\\Queue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yaochufa\\Queue\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "php-cs-fixer fix --config=./.php_cs"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
